{
  "name": "todo-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run json-server\" \"npm run ng-serve-dev\"",
    "json-server": "json-server-auth db.json --port 8000",
    "ng-serve-dev": "ng serve --configuration=development --port 4000",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick --staged",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@ngrx/effects": "^16.3.0",
    "@ngrx/store": "^16.3.0",
    "@ngrx/store-devtools": "^16.3.0",
    "bootstrap": "^5.2.3",
    "ngx-bootstrap": "^11.0.2",
    "ngx-cookie-service": "^16.1.0",
    "ngx-toastr": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.10",
    "@angular-eslint/builder": "16.3.1",
    "@angular-eslint/eslint-plugin": "16.3.1",
    "@angular-eslint/eslint-plugin-template": "16.3.1",
    "@angular-eslint/schematics": "16.3.1",
    "@angular-eslint/template-parser": "16.3.1",
    "@angular/cli": "^16.2.10",
    "@angular/compiler-cli": "^16.2.0",
    "@ngrx/eslint-plugin": "^16.3.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.51.0",
    "husky": "^8.0.0",
    "json-server": "^0.17.4",
    "json-server-auth": "^2.1.0",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "typescript": "~5.1.3"
  }
}
